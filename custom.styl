/* All events page styles
-------------------------------------- */
#the-event-loop
  margin-left 0
  display table
  
.event-container
  // display inline-block
  list-style none
  width 32%
  margin-bottom 40px
  float left

.event-container:nth-child(3n+2)
  margin-right 10px
  margin-left 10px
  
.event-img
  max-height 254px
  overflow hidden

.event-img img
  width 100% !important
  margin-bottom 0 !important

.event-meta
  background-color #900025
  padding 20px
  
.event-meta a
   color #ffffff
  
.event-title h2
  font-size 22px
  line-height 26px

@media screen and (max-width 768px)
  #event
    width 100%
    
